   <class type="muse_amcl::MockUpdate" base_class_type="muse_amcl::Update">
     <description>Mockup</description>
   </class>
   <class type="muse_amcl::MockPropagation" base_class_type="muse_amcl::Propagation">
     <description>Mockup</description>
   </class>
   <class type="muse_amcl::MockDataProvider" base_class_type="muse_amcl::DataProvider">
     <description>Mockup</description>
   </class>
   <class type="muse_amcl::MockMapProvider" base_class_type="muse_amcl::MapProvider">
   	 <description>Mockup</description>
   </class>
